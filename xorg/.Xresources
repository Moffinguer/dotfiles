!! scroll in relation to buffer (with mouse scroll, Shift+Page Up or Shift+Fn+Up Arrow)
URxvt.scrollWithBuffer: true

!! Disable confirm Pase
URxvt.confirmPaste: false

!! cliboard
URxvt.keysym.C-S-C: eval:selection_to_clipboard
URxvt.keysym.C-S-V: eval:paste_clipboard
URxvt.iso14755: false
Urxvt.iso14755_52: false

!! Resize font
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
URxvt.resize-font.step: 2

URxvt*transparent: true
URxvt*shading: 10

!! Window geometry
URxvt.geometry: 400x400

!! disable automatic scroll with less similars
URxvt.secondaryScroll: 1
URxvt.secondaryWheel: 1

!! Disable scrollbar
URxvt.scrollBar: false

!! Font
URxvt*font: xft:Terminess Nerd Font:pixelsize=17,xft:FiraCode Nerd Font Mono:pixelsize=14 # 3 size less Fira

!! Colors
! Special
URxvt.background: #080808
URxvt.foreground: #BDBDBD

! black
URxvt.color0: #323437
URxvt.color8: #949494

! red
URxvt.color1: #FF5454
URxvt.color9: #FF5189

! green
URxvt.color2:  #8CC85F
URxvt.color10: #36C692

! yellow
URxvt.color3:  #E3C78A
URxvt.color11: #C2C292

! blue
URxvt.color4:  #80A0FF
URxvt.color12: #74B2FF

! magenta
URxvt.color5:  #CF87E8
URxvt.color13: #AE81FF

! cyan
URxvt.color6:  #79DAC8
URxvt.color14: #85DC85

! white
URxvt.color7:  #C6C6C6
URxvt.color15: #E4E4E4

!!!! Extensions
!URxvt.perl-ext-common: default, matcher, url-select, resize-font, selection-autotransform

!! Clickable URL
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.colorUL: #4682B4
URxvt.url-select.underline: true
URxvt.matcher.button: 1
