!! scroll in relation to buffer (with mouse scroll, Shift+Page Up or Shift+Fn+Up Arrow)
URxvt.scrollWithBuffer: true

!! Disable confirm Pase
URxvt.confirmPaste: false

!! cliboard
URxvt.keysym.C-S-C: eval:selection_to_clipboard
URxvt.keysym.C-S-V: eval:paste_clipboard
URxvt.iso14755: false
Urxvt.iso14755_52: false

!! Resize font
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
URxvt.resize-font.step: 2


URxvt*transparent: true
URxvt*shading: 10

!! Window geometry
URxvt.geometry: 400x400

!! disable automatic scroll with less similars
URxvt.secondaryScroll: 1
URxvt.secondaryWheel: 1

!! Disable scrollbar
URxvt.scrollBar: false

!! Fullscreen switch
URxvt.keysym.F11:  perl:fullscreen:switch

!! Font
URxvt*font: xft:IBM 3270:size=12, xft:Iosevka Nerd Font Mono:size=12

!! Colors
! Special
URxvt.background: #0E1111
URxvt.foreground: #C6C6C4

! black
URxvt.color0: #0E1111
URxvt.color8: #696969

! red
URxvt.color1: #B6403A
URxvt.color9: #FF2400

! green
URxvt.color2:  #478226
URxvt.color10: #03C03C

! yellow
URxvt.color3:  #FFFF00
URxvt.color11: #FDFF00

! blue
URxvt.color4:  #778899
URxvt.color12: #007FFF

! magenta
URxvt.color5:  #9A4EAE
URxvt.color13: #FF1493

! cyan
URxvt.color6:  #20B2AA
URxvt.color14: #00CCCC

! white
URxvt.color7:  #FFFFFF
URxvt.color15: #FFFAFA

!!!! Extensions
URxvt.perl-ext-common: default, matcher, url-select, resize-font, selection-autotransform, fullscreen 

!! Clickable URL
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.colorUL: #4682B4
URxvt.url-select.underline: true
URxvt.matcher.button: 1
